{
  "name": "jbaron/fal_database",
  "description": "TYPO3 FAL storage driver for storing files in the database",
  "type": "typo3-cms-extension",
  "license": "MIT",
  "keywords": [
    "TYPO3 CMS",
    "FAL",
    "Database"
  ],
  "authors": [
    {
      "name": "Jost Baron",
      "email": "jost.baron@mein-bauernhof.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jbaron\\FalDatabase\\": "Classes/"
    }
  },
  "require": {
    "typo3/cms-core": "^10.4 || ^11.5",
    "php": "^7.4 || ^8.0 || ^8.1"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "fal_database"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
